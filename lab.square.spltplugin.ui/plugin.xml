<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <category
            id="lab.square.spltplugin.ui.commands.category"
            name="Sample Category">
      </category>
      <command
            categoryId="lab.square.spltplugin.ui.commands.category"
            name="Sample Command"
            id="lab.square.spltplugin.ui.commands.sampleCommand">
      </command>
      <command
            id="lab.square.spltplugin.ui.commands.importSessionCommand"
            name="Import Session">
            <commandParameter
               id="lab.square.spltplugin.ui.params.viewId"
               name="View Id"
               optional="false">
         </commandParameter>
      </command>
      <command
            id="lab.square.spltplugin.ui.commands.openSessionCommand"
            name="Open Session">
            <commandParameter
               id="lab.square.spltplugin.ui.params.viewId"
               name="View Id"
               optional="false">
         </commandParameter>
         <commandParameter
               id="lab.square.spltplugin.ui.params.sessionId"
               name="Session Id"
               optional="false">
         </commandParameter>
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="lab.square.spltplugin.ui.handlers.SampleHandler"
            commandId="lab.square.spltplugin.ui.commands.sampleCommand">
      </handler>
      <handler
            class="lab.square.spltplugin.ui.handlers.ImportSessionHandler"
            commandId="lab.square.spltplugin.ui.commands.importSessionCommand">
      </handler>
      <handler
            class="lab.square.spltplugin.ui.handlers.OpenSessionHandler"
            commandId="lab.square.spltplugin.ui.commands.openSessionCommand">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="lab.square.spltplugin.ui.commands.sampleCommand"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               id="lab.square.spltplugin.ui.menus.sampleMenu"
               label="Sample Menu"
               mnemonic="M">
            <command
                  commandId="lab.square.spltplugin.ui.commands.sampleCommand"
                  id="lab.square.spltplugin.ui.menus.sampleCommand"
                  mnemonic="S">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="lab.square.spltplugin.ui.toolbars.sampleToolbar">
            <command
                  id="lab.square.spltplugin.ui.toolbars.sampleCommand"
                  commandId="lab.square.spltplugin.ui.commands.sampleCommand"
                  icon="icons/sample.png"
                  tooltip="Say hello world">
            </command>
         </toolbar>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:lab.square.spltplugin.ui.spltCoverageTable?after=additions">
         <command
               commandId="lab.square.spltplugin.ui.commands.importSessionCommand"
               icon="icons/sample.png"
               style="push">
            <parameter
                  name="lab.square.spltplugin.ui.params.viewId"
                  value="lab.square.spltplugin.ui.spltCoverageTable">
            </parameter>
         </command>
         <command
               commandId="lab.square.spltplugin.ui.commands.openSessionCommand"
               icon="icons/sample.png"
               id="lab.squarte.spltplugin.ui.menus.sessionList"
               style="pulldown">
         </command>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:lab.square.spltplugin.ui.spltCoverageTree?after=additions">
         <command
               commandId="lab.square.spltplugin.ui.commands.importSessionCommand"
               icon="icons/sample.png"
               style="push">
            <parameter
                  name="lab.square.spltplugin.ui.params.viewId"
                  value="lab.square.spltplugin.ui.spltCoverageTree">
            </parameter>
         </command>
         <command
               commandId="lab.square.spltplugin.ui.commands.openSessionCommand"
               icon="icons/sample.png"
               id="lab.square.spltplugin.ui.menus.sessionList"
               style="pulldown">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:lab.square.spltplugin.ui.menus.sessionList">
         <dynamic
               class="lab.square.spltplugin.ui.dynamic.SessionListDynamic"
               id="lab.square.spltplugin.ui.dynamic2">
         </dynamic>
      </menuContribution>
   </extension>
   <extension
         id="lab.square.plugin.markers.covered"
         point="org.eclipse.core.resources.markers">
   </extension>
   <extension
         point="org.eclipse.ui.editors.annotationTypes">
      <type
            markerType="lab.square.spltplugin.markers.covered"
            name="lab.square.spltplugin.ui.annotationType.covered">
      </type>
   </extension>
   <extension
         point="org.eclipse.ui.editors.markerAnnotationSpecification">
      <specification
            annotationType="lab.square.spltplugin.ui.annotationType.covered"
            colorPreferenceKey="covered_color"
            colorPreferenceValue="192,255,192"
            contributesToHeader="true"
            highlightPreferenceKey="covered_highlight"
            highlightPreferenceValue="true"
            label="Line Covered"
            overviewRulerPreferenceKey="covered_overviewruler"
            overviewRulerPreferenceValue="false"
            textPreferenceKey="covered_text"
            textPreferenceValue="false"
            textStylePreferenceValue="IBEAM"
            verticalRulerPreferenceKey="covered_verticalruler"
            verticalRulerPreferenceValue="true">
      </specification>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="lab.square.spltplugin.ui.spltCoverageCategory"
            name="SPLTCoverage">
      </category>
      <view
            allowMultiple="false"
            category="lab.square.spltplugin.ui.spltCoverageCategory"
            class="lab.square.spltplugin.ui.views.SpltCoverageTable"
            id="lab.square.spltplugin.ui.spltCoverageTable"
            name="SPLTCoverageTable"
            restorable="true">
      </view>
      <view
            allowMultiple="false"
            category="lab.square.spltplugin.ui.spltCoverageCategory"
            class="lab.square.spltplugin.ui.views.SpltCoverageTree"
            id="lab.square.spltplugin.ui.spltCoverageTree"
            name="SPLTCoverageTree"
            restorable="true">
      </view>
   </extension>

</plugin>
